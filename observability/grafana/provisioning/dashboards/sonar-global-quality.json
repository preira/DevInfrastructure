        "sizeField": "debt_days",
        "colorByField": "top10",
        "labelFields": [
          "project_name"
        ]
      },
      "transformations": [
        {
          "id": "joinByField",
          "options": {
            "byField": "project_key",
            "mode": "outer"
          }
        },
        {
          "id": "organize",
          "options": {
            "excludeByName": {
              "Time": true,
              "instance": true,
              "job": true,
              "project_name 2": true
            },
            "renameByName": {
              "project_key 1": "project_key",
              "project_name 1": "project_name",
              "Value": "debt_days",
              "Value 1": "top10",
              "Value #A": "debt_days",
              "Value #B": "top10"
            }
          }
        }
      ],
      "targets": [
        {
          "editorMode": "code",
          "expr": "sort_desc(((sonar_project_sqale_index unless sonar_project_sqale_index != sonar_project_sqale_index)) / 60 / 8)",
          "format": "table",
          "instant": true,
          "legendFormat": "{{project_name}}",
          "range": false,
          "refId": "A"
        },
        {
          "editorMode": "code",
          "expr": "(\n  (\n    ((sonar_project_sqale_index unless sonar_project_sqale_index != sonar_project_sqale_index)) / 60 / 8\n  )\n  >= bool scalar(\n    quantile(\n      0.9,\n      ((sonar_project_sqale_index unless sonar_project_sqale_index != sonar_project_sqale_index)) / 60 / 8\n    )\n  )\n)",
          "format": "table",
          "instant": true,
          "legendFormat": "top_10_percent",
          "range": false,
          "refId": "B"
        }
